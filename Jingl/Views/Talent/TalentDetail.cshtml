    <script type="text/javascript">
        // When the user scrolls down 20px from the top of the document, show the button
        window.onscroll = function () { scrollFunction() };

        function scrollFunction() {
            if (document.body.scrollTop > 5000 || document.documentElement.scrollTop > 5000) {
                document.getElementById("btnTop").style.display = "block";
            } else {
                document.getElementById("btnTop").style.display = "none";
            }
        }

        // When the user clicks on the button, scroll to the top of the document
        function BackTop() {
            document.body.scrollTop = 0; // For Safari
            document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
        }
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#playProfVid").click(function () {

                if (document.getElementById("adVIdeo").paused) {
                    document.getElementById("adVIdeo").play();
                    document.getElementById("playBtn").style.display = "none";
                    document.getElementById("pauseBtn").style.display = "inline";
                } else {
                    document.getElementById("adVIdeo").pause();
                    document.getElementById("playBtn").style.display = "inline";
                    document.getElementById("pauseBtn").style.display = "none";
                }
            });


            var video = document.getElementById('adVIdeo');

            // When the 'ended' event fires
            video.addEventListener('ended', function () {
                // Reset the video to 0
                video.currentTime = 0;
                // And play again
                document.getElementById("playBtn").style.display = "inline";
                document.getElementById("pauseBtn").style.display = "none";
            });
        });
    </script>

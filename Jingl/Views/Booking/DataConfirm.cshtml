                                    $("#divgopay").hide();
                                }
                                else {
                                    $("#divovo").show();
                                    $("#divgopay").show();
                                }
                            }
                            else {
                                $("#divbank").show();
                                $("#divovo").show();
                            }

                          //  document.getElementById('diskon').style.display = 'none';
                        }
                    }
                })
            }
            else {
                var message = $("#message");
                var potongan = $("#Potogan");
                message.css("color", "red");
                message.html("");
                potongan.val(0);
                lblTotalTobePaid.text(Number(PriceAmount).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,'));
                $("#lblPotongan").text(0);
                $("#isValidVoucher").val(1);

                if (Number(jumlahbayar) < 10000) {
                    //$('#yourid').attr('name').prop(,)

                    $("#divbank").hide();

                    if (Number(jumlahbayar) == 0)
                    {
                        if (Number(jumlahbayar) == 0) {
                            $("#divovo").hide();
                            $("#divgopay").hide();
                        }
                        else {
                            $("#divovo").show();
                            $("#divgopay").show();
                        }
                    }
                }
                else {
                    $("#divbank").show();
                }
               // document.getElementById('diskon').style.display = 'none';
            }
        }
    </script>
}



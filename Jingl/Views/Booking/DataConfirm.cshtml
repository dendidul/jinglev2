    <script type="text/javascript">
        function checkVoucherCode() {
            var VoucherCd = $("#VoucherCd").val();
            var PriceAmount = $("#TotalPay").text();
            
            //var BookId = $("#BookId").val();
            var url = '@Url.Action("VerifyVoucherCode", "Booking")';
            //console.log(VoucherCd);
            if (VoucherCd != '' && VoucherCd != null) {
                $.ajax({
                    type: "POST",
                    url: url,
                    data: {
                        BookId: @Model.Id,
                        VoucherCd: VoucherCd,
                        PriceAmount: PriceAmount
                    },
                    success: function (data) {
                        var message = $("#message");
                        var potongan = $("#Potogan");
                        var lblPotongan = $("#lblPotongan");

                        if (data > 0) {

                            message.css("color", "green");
                            message.html("Voucher is valid");
                            lblPotongan.addClass("amountlines");
                            lblPotongan.html(data.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,'));
                            potongan.val(data);
                            var jumlahbayar = Number(PriceAmount) - data;
                            $("#lblTotalTobePaid").html(jumlahbayar.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,'));
                            //console.log(data);
                            //console.log(potongan.val());
                            $("#isValidVoucher").val(1);
                            document.getElementById('diskon').style.display = 'block';

                            if (Number(jumlahbayar) < 10000) {
                                //$('#yourid').attr('name').prop(,)
                                $("#divbank").hide();
                                if (Number(jumlahbayar) == 0) {
                                    $("#divovo").hide();
                                    $("#divgopay").hide();
                                }
                                else {
                                    $("#divovo").show();
                                    $("#divgopay").show();
                                }
                            }
                            else {
                                $("#divbank").show();
                                $("#divovo").show();
                                $("#divgopay").show();
                            }

                        }
                        else {

                            message.css("color", "red");
                            message.html("Voucher is NOT valid");
                            potongan.val(data);
                            $("#lblPotongan").text(0);
                            $("#lblTotalTobePaid").html(Number(PriceAmount).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,'));
                          //  $("#lblTotalTobePaid").text(PriceAmount);
